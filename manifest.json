{
  "name": "iFrame Ticket Sidebar",
  "version": "2.0.7",
  "defaultLocale": "en",
  "private": false,
  "frameworkVersion": "2.0",
  "author": {
    "name": "XVII Digital",
    "email": "zendesk+app@xvii.com.au",
    "url": "https://xvii.au"
  },
  "location": {
    "support": {
      "ticket_sidebar": {
        "url": "assets/iframe.html",
        "flexible": true
      },
      "background": "assets/background.html"
    }
  },
  "parameters": [
    {
      "name": "iframeURL",
      "type": "url",
      "required": true
    },
    {
      "name": "iframeHeight",
      "type": "number",
      "required": true,
      "default": 300
    },
    {
      "name": "zafSdkSupport",
      "type": "checkbox",
      "default": "1"
    }
  ]
}